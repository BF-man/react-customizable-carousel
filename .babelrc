{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
  ],
  "env": {
    "es": {
      "presets": [
        ["@babel/preset-env", { "modules": false, "useBuiltIns": "entry" }],
        "@babel/preset-react"
      ]
    },
  }
}
