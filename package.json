{
  "name": "react-customizable-carousel",
  "version": "0.0.7",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "style": "./styles/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/BF-man/react-customizable-carousel"
  },
  "author": "Bubnov Artem (https://github.com/BF-man)",
  "license": "MIT",
  "scripts": {
    "dev-server": "webpack-dev-server --hot --port=8080 --output-public-path=http://localhost:8080/ --config ./webpack.config.dev.js --content-base",
    "build": "yarn build:lib && yarn build:es && yarn build:css",
    "build:lib": "babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:css": "postcss src/*.css --output styles/index.css --config postcss.config.js",
    "demo": "webpack --config ./webpack.config.js",
    "clearall": "rimraf dist styles lib es demo"
  },
  "files": [
    "es",
    "lib",
    "styles",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^15"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "react-icons": "^2.2.5",
    "react-measure": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.11.2",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.29.0",
    "normalize.css": "^5.0.0",
    "path-complete-extname": "^0.1.0",
    "postcss": "^5.2.13",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.1",
